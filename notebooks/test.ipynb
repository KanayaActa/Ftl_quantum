    "import numpy as np\n",
    "from qiskit import QuantumCircuit\n",
    "qc = QuantumCircuit(2)\n",
    "qc.x(0)\n",
    "qc.h([0,1])\n",
    "qc.rx(theta=np.pi/4,qubit=0)\n",
    "qc.cx(0,1) # CNOT\n",
    "qc.measure_all() # Measure all qubits\n",
    "qc.draw(\"mpl\")\n"
   ]
